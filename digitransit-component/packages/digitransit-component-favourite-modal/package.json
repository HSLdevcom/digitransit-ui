  "license": "(AGPL-3.0 OR EUPL-1.2)",
  "dependencies": {
    "@digitransit-component/digitransit-component-icon": "^0.0.9",
    "@hsl-fi/sass": "^0.1.1",
    "@hsl-fi/modal": "^0.1.7",
    "@hsl-fi/site-header": "^0.2.1"
  },
  "peerDependencies": {
    "classnames": "2.2.6",
    "i18next": "^19.3.3",
    "lodash": "4.17.14",
    "lodash-es": "4.17.14",
    "prop-types": "^15.7.2",
    "react": "^16.13.0"
  }

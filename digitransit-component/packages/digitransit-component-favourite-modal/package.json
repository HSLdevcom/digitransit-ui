    "watch": "ENV=development webpack --watch --config webpack.config.babel.js",
    "clean": "rm -r ./lib",
    "test": "mocha -r esm test.generated",
    "docs": "node -r esm ../../scripts/generate-readmes"
  },
  "repository": {
    "type": "git",

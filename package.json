{
  "name": "digitransit-ui",
  "version": "0.0.1",
  "description": "Digitransit UI - open nationwide journey planning platform",
  "author": "digitransit@hsl.fi",
  "license": "(AGPL-3.0 OR EUPL-1.2)",
  "private": true,
  "workspaces": [
    "digitransit-util/packages/*"
  ],
  "scripts": {
    "static": "mkdir -p _static/js; mkdir -p _static/css; cp -r static/* _static",
    "win-static": "xcopy static\\* _static /e /s /y /i& if not exist _static\\js mkdir _static\\js& if not exist _static\\css mkdir _static\\css",
    "prebuild": "yarn run static",
    "predev": "yarn run static",
    "predev-win-hsl": "yarn run win-static",
    "predev-win-national": "yarn run win-static",
    "dev": "PID=$$; trap 'pkill -QUIT -P $PID > /dev/null' EXIT; NODE_ENV=development nodemon -e js,css,scss,html --watch ./server/ --watch ./app/ server/server.js & NODE_ENV=development webpack-dev-server",
    "dev-win-hsl": "cd win-launch-scripts&& hsl-win-launch-script.bat",
    "dev-win-national": "cd win-launch-scripts&& set CONFIG=&& national-win-launch-script.bat",
    "dev-nowatch": "NODE_ENV=development node server/server.js & NODE_ENV=development webpack-dev-server",
    "test-local": "./test/flow/script/run-ui-tests.sh local",
    "test-browserstack": "./test/flow/script/run-ui-tests.sh browserstack",
    "test-coverage": "NODE_ENV=test nyc yarn run test-unit",
    "test-report": "codecov",
    "test-smoke": "./test/flow/script/run-ui-tests.sh smoke",
    "test-saucelabs": "./test/flow/script/run-ui-tests.sh saucelabs",
    "test-unit": "mocha --require mock-local-storage --file test/unit/helpers/babel-register.js --file test/unit/helpers/init.js \"test/unit/**/*.test.js\"",
    "test-single": "mocha --require mock-local-storage --file test/unit/helpers/babel-register.js --file test/unit/helpers/init.js \"test/unit/**/*.test.js\" -g $1",
    "test-visual": "gemini --reporter flat test test/visual/",
    "test-visual-update": "gemini update test/visual/",
    "es-lint": "eslint .",
    "lint": "yarn run es-lint && yarn run stylelint && yarn lint-translations",
    "lint-translations": "yarn jsonlint --sort-keys --in-place app/translations",
    "stylelint": "stylelint sass/**/*.scss app/**/*.scss --syntax scss",
    "build": "NODE_ENV=production node --max_old_space_size=4096 node_modules/.bin/webpack --progress --color --display-error-details",
    "win-build": "set NODE_ENV=production&& webpack --progress --color",
    "start": "NODE_ENV=production node $NODE_OPTS server/server",
    "win-start": "set NODE_ENV=production&& set CONFIG=hsl&& node server/server.js",
    "add-theme": "node build/add-theme",
    "digitransit-util-changed": "lerna changed",
    "digitransit-util-docs": "node -r esm ./digitransit-util/scripts/generate-readmes",
    "digitransit-util-list": "lerna list",
    "digitransit-util-new-module": "node -r esm ./digitransit-util/scripts/create-new-module $1",
    "digitransit-util-publish": "lerna publish --no-git-tag-version",
    "digitransit-util-tests": "yarn workspaces run test"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/HSLdevcom/digitransit-ui"
  },
  "bugs": {
    "url": "https://github.com/HSLdevcom/digitransit-ui/issues"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.11.2",
    "npm": ">=3.0"
  },
  "browserslist": [
    "IE 11",
    "> 0.2% in FI",
    "not op_mini all"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.scss": [
      "stylelint --syntax scss"
    ]
  },
  "git-precommit-checks": {
    "rules": [
      {
        "message": "You have unresolved conflict markers",
        "regex": "/^[<>|=]{4,}/m"
      },
      {
        "filter": "\\.js$",
        "nonBlocking": "true",
        "message": "You are using `console.log` in your JavaScripts",
        "regex": "console\\.log"
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && git-precommit-checks",
      "pre-push": "npm run test-unit"
    }
  },

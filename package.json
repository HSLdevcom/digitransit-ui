{
  "name": "digitransit-ui",
  "version": "0.0.1",
  "description": "Digitransit UI - open nationwide journey planning platform",
  "author": "digitransit@hsl.fi",
  "license": "(AGPL-3.0 OR EUPL-1.2)",
  "scripts": {
    "static": "mkdir -p _static/js; mkdir -p _static/css; cp -r static/* _static",
    "predev": "npm run static",
    "prestart": "npm run static",
    "prewin-start": "if not exist _static\\js mkdir _static\\js&& if not exist _static\\css mkdir _static\\css&& xcopy static\\* _static /e /s",
    "dev": "NODE_ENV=development nodemon -e js,jsx,cjsx,css,scss,html,coffee --watch ./app/ server/server.js & NODE_ENV=development node server/hotLoadServer.js",
    "dev-win-hsl": "cd win-launch-scripts&& hsl-win-launch-script.bat",
    "dev-win-national": "cd win-launch-scripts&& set CONFIG=&& national-win-launch-script.bat",
    "dev-nowatch": "NODE_ENV=development node server/server.js & NODE_ENV=development node server/hotLoadServer.js",
    "test-local": "./test/script/run-ui-tests.sh local",
    "test-browserstack": "./test/script/run-ui-tests.sh browserstack",
    "test-acceptance": "./acceptance-tests/script/run-acceptance-tests.sh",
    "lint": "node_modules/.bin/coffeelint -f coffeelint.json --ext=cjsx,coffee app",
    "build": "NODE_ENV=production webpack -p --progress --color --display-error-details",
    "win-build": "set NODE_ENV=production&& webpack -p --progress --color",
    "start": "NODE_ENV=production node server/server",
    "win-start": "set NODE_ENV=production&& set CONFIG=hsl&& node server/server.js"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/HSLdevcom/digitransit-ui"
  },
  "bugs": {
    "url": "https://github.com/HSLdevcom/digitransit-ui/issues"
  },
  "browser": {
    "./app/config.coffee": "./app/config.client.coffee"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=5.0",
    "npm": ">=3.0"
  },
  "dependencies": {
    "babel": "6.5.1",
    "babel-core": "6.5.1",
    "babel-plugin-transform-class-properties": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.5.1",
    "babel-relay-plugin": "0.6.3",
    "body-parser": "1.14.2",
    "classnames": "2.2.3",
    "cookie-parser": "1.4.1",
    "debug": "2.2.0",
    "express": "4.13.4",
    "fluxible": "1.0.3",
    "fluxible-action-utils": "0.2.4",
    "fluxible-addons-react": "0.2.3",
    "foundation-apps": "1.2.0",
    "history": "1.13.0",
    "javascript-natural-sort": "0.7.1",
    "leaflet": "1.0.0-beta.2",
    "lodash": "4.3.0",
    "material-ui": "0.14.4",
    "moment": "2.11.2",
    "mqtt": "1.7.0",
    "node-cjsx": "1.0.0",
    "node-fetch": "1.3.3",
    "pbf": "^1.3.5",
    "polyfill-service": "3.2.0",
    "polyline-encoded": "0.0.7",
    "raven": "0.10.0",
    "raven-js": "2.1.0",
    "react": "0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-autowhatever": "3.0.0",
    "react-cookie": "^0.4.3",
    "react-dom": "0.14.7",
    "react-helmet": "2.3.1",
    "react-intl": "2.0.0-beta-2",
    "react-leaflet": "0.10.0",
    "react-relay": "0.6.1",
    "react-router": "1.0.2",
    "react-router-relay": "0.8.0",
    "react-simpletabs": "0.7.0",
    "react-slick": "0.10.0",
    "react-tap-event-plugin": "0.2.2",
    "serialize-javascript": "1.1.2",
    "slick-carousel": "1.5.9",
    "vector-tile": "1.2.0",
    "zurb-foundation-5": "5.4.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-dev-expression": "^0.1.0",
    "chance": "^0.8.0",
    "cjsx-loader": "^2.1.0",
    "coffee-loader": "^0.7.2",
    "coffeelint": "^1.14.2",
    "css-loader": "^0.23.1",
    "csswring": "^4.2.1",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gherkin-mocha": "^0.0.5",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "nightwatch": "^0.8.16",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "selenium-webdriver": "^2.48.2",
    "sigmund": "^1.0.1",
    "stats-webpack-plugin": "^0.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^2.0.7-beta",
    "webpack-dev-server": "^1.14.1"
  }
}

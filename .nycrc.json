{
  "all": true,
  "sourceMap": false,
  "instrument": false,
  "require": [
    "@babel/register"
  ],
  "extensions": [
    ".js",
    ".ts",
    ".tsx"
  ],
  "include": [
    "app/**/*.js"
  ],
  "exclude": [
    "**/*.d.ts"
  ],
  "reporter": [
    "lcovonly",
    "html",
    "text-summary"
  ]
}
